{
  "name": "vs-review",
  "displayName": "vs-review",
  "description": "Review",
  "version": "0.0.1",
  "main": "./extension/out/extension.js",
  "activationEvents": [
    "onCommand:vsr.start",
    "onCommand:vsr.merge-request.show"
  ],
  "engines": {
    "vscode": "^1.33.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "vsr.start",
        "title": "Start VS Review"
      },
      {
        "command": "vsr.merge-request.show",
        "title": "VSR MergeRequest"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "cd extension && yarn run compile",
    "postinstall": "cd extension && node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "lerna": "^3.13.4"
  }
}
